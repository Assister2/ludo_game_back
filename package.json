{
  "name": "ludo_backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "startDocker": "node ./bin/www.js",
    "startDockerBuild": "node ./built/index.js",
    "start": "nodemon app.js",
    "staging": "nodemon node ./built/index.js",
    "build": "webpack --mode development --stats-error-details --config webpack.config.js",
    "esbuild-build": "node -r dotenv/config ./node_modules/esbuild/bin/esbuild app.js --platform=node --bundle --outfile=built/index.js && copy .env built"
  },
  "dependencies": {
    "@sentry/node": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "connect-redis": "^7.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^10.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.13.15",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.3.0",
    "node-telegram-bot-api": "^0.61.0",
    "nodemon": "^3.0.1",
    "redis": "^4.6.7",
    "socket.io": "^4.5.4",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-loader": "^9.1.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild": "^0.18.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
